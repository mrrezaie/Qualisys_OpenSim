Project ID: Open PAF OpenSim Example
Package Information:
  Name: OpenSim Example
  Version: 1.0.0
  Required QTM version: 2.16.3600
  Encoding bytes: €€€€€

Root type: Subject
Default fields: [Creation date, Creation time]

Date format: Iso
Date separator: Dash
Filename filter: \.(pdf)$

Types:
  Subject:
    Participant:
      Fields: [ID, Name, Gender, Age, Weight, Height]
      Children: [Session]
      Directory pattern: $ID$_$Name$

  Session:
    Session:
      Fields: [Condition, Operator, GRF cutoff frequency, Comments]
      Measurements: [Static, Gait]
      Analyses: [OpenSim Workflow]
      Directory pattern: $Creation date$_$Condition$

Measurements:
  Static:
    Measurement length: 1
    Measurement type: Static
  Gait:
    Maximum count: 10
    Count: 3
    Measurement type: Dynamic

Analyses:
  OpenSim Workflow:
    Type: External program
    Program display name: Python
    Export session: Yes
    Export measurements: mat
    Arguments: [$TemplateDirectory$OSIM.py, $WorkingDirectory$__________]
    Do not wait for Application: True



Fields:
  ID:
    Type: String
    Inherit: Connect
  Name:
    Type: String
    Inherit: Connect
    Force: Yes
  Condition:
    Type: String
    Inherit: Connect
    Force: Yes
  Age:
    Type: Float
    Decimals: 0
    Inherit: Connect
    Force: Yes
  GRF cutoff frequency:
    Type: Float
    Decimals: 0
    Quantity: Frequency
    Unit: hertz
    Max: 55
    Min: 5
    Default: 20
    Inherit: Connect
  Creation date:
    Type: Date
    Readonly: Yes
  Creation time:
    Type: TimeOfDay
    Readonly: Yes
  Session ID:
    Type: String
    Inherit: Connect
    Default: 01
  Operator:
    Type: String
    Inherit: Copy
  Height:
    Type: Float
    Quantity: Length
    Unit: centimeters
    Decimals: 1
    Inherit: Connect
    Max: 250
    Min: 50
    Force: No
  Weight:
    Type: Float
    Quantity: Mass
    Unit: kilograms
    Decimals: 2
    Inherit: Connect
    Max: 250
    Min: 0
    Force: Yes
  Gender:
    Type: Enum
    Values: [Unspecified, Female, Male]
    Inherit: Connect
    Default: Unspecified
  Measurement type:
    Type: String

Columns:
  Default column definitions:
    ID:
      Width: 200
      Fields:
        Default: Filename
        Participant: ID
    Name:
      Width: 150
      Fields: 
        Participant: Name
    Created:
      Width: 65
      Field: Creation date
    Comments:
      Field: Comments
      Width: 500